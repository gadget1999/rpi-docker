    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Nook Weather",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
              ""
            ],
            "env": {
              "WEATHER_API_PROVIDER_1":"NWS",
              "WEATHER_API_KEY_1":"nook-weather",
              "GPS_COORDINATES": "40.69,-74.26"
            },
            "type": "python"            
        },
        {
          "name": "Python: WebMonitor",
          "request": "launch",
          "program": "${file}",
          "args": [
            "${relativeFileDirname}/web-monitor/web-monitor.cfg"
          ],
          "env": {
            "SENDGRID_API_KEY": "INVALID_KEY_PLACE_HOLDER"
          },
          "console": "integratedTerminal",
          "type": "python"
        }
    ]
}